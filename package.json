{
  "name": "my-first-react-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/CristyTarantino/reactjs-blog",
  "dependencies": {
    "axios": "0.18.1",
    "history": "4.7.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.1",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
